{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "no-alert": ["error"],
    "quotes": ["off"], 
    "prettier/prettier": "error"
  },
  "globals": {
    "window": true,
    "document": true
  }
}
