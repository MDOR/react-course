{
  "name": "react-course",
  "version": "0.0.1",
  "description": "This course is intended for people who want to learn React, in this package we will provide several examples about how to learn this technology",
  "main": "index.js ",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "format": "prettier *.js app/*.{jsx,js} src/**/*.{jsx,js} --write",
    "lint": "eslint --ext .jsx,.js ./ src/",
    "will-commit": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MDOR/react-course.git"
  },
  "keywords": [
    "react",
    "course",
    "javascript",
    "es6",
    "babel",
    "react-router",
    "eslint"
  ],
  "author": "Marco Antonio Dominguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MDOR/react-course/issues"
  },
  "homepage": "https://github.com/MDOR/react-course#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-object-rest-spread": "0.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "sass-loader": "^7.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
